<Window x:Class="Up.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Up"
        mc:Ignorable="d"
        Title="Лаборатория" Height="700" Width="1200" Icon="/logo.png">
    <Grid>

        <Frame  NavigationUIVisibility='Hidden' Name="MainFrame" Background="#FFC6D7FF"></Frame>
        <TextBox x:Name="Time" Text="{Binding TickCounter,
                                  RelativeSource={RelativeSource FindAncestor,
                                                                 AncestorType={x:Type local:MainWindow}}}" HorizontalAlignment="Left" Margin="913,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="40" Width="58" FontSize="22" Foreground="Black" TextChanged="TextBox_TextChanged" IsEnabled="False" BorderBrush="{x:Null}" Background="{x:Null}" SelectionBrush="{x:Null}"/>
        <Label Content="Мин до кварцевания" Margin="958,16,0,0" HorizontalAlignment="Left"   VerticalAlignment="Top" Height="40" Width="250" FontSize="22" Foreground="Black"  IsEnabled="False" BorderBrush="{x:Null}" Background="{x:Null}"/>
    </Grid>
</Window>
